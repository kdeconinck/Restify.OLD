# Indicate that this is the `ROOT` configuration file.
root = true

# Use `CR LF` lines endings and end every file with a final newline.
[*]
end_of_line = crlf
insert_final_newline = true

# Configure C# source code files.
[*.cs]
dotnet_style_qualification_for_field    = true:warning # Section: Usage of the `this.` qualifier.
